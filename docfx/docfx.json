{
  "metadata": [
    {
      "src": [
        {
          "src": "../NF.Tool.ReleaseNoteMaker/",
          "files": [
            "**/*.sln"
          ]
        }
      ],
      "allowCompilationErrors": true,
      "filter": "filterConfig.yml",
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "NF.Tool.ReleaseNoteMaker",
      "_appTitle": "NF.Tool.ReleaseNoteMaker",
	  "_googleAnalyticsTagId": "G-855BJNV9VD",
      "_enableSearch": true,
      "pdf": false
    }
  }
}